<?xml version="1.0" encoding="utf-8"?>
<vega>
  <input>
    <type>FileSMI</type>
    <source>%s</source>
  </input>
  <models>
    <model>muta_caesar</model>
    <!--  QSAR classification model for Mutagenicity (from CAESAR project) ver.2.1.13
-->
    <model>carc_caesar</model>
    <!--  QSAR classification model for Carcinogenicity (from CAESAR project) ver.2.1.9
-->
    <model>devtox_caesar</model>
    <!--  QSAR classification model for Developmental Toxicity (from CAESAR project) ver.2.1.7
-->
    <model>rba_irfmn</model>
    <!--  Classification model for Estrogen Receptor Relative Binding Affinity (IRFMN) ver.1.0.1
-->
    <model>skin_caesar</model>
    <!--  QSAR classification model for Skin sensitization (from CAESAR project) ver.2.1.6
-->
    <model>fathead_epa</model>
    <!--  QSAR model for Fathead Minnow LC50 96h (from EPA T.E.S.T software) ver.1.0.7
-->
    <model>daphnia_epa</model>
    <!--  QSAR model for Daphnia Magna LC50 48h (from EPA T.E.S.T software) ver.1.0.7
-->
    <model>bcf_caesar</model>
    <!--  QSAR regression model for fish BCF (from CAESAR project) ver.2.1.14
-->
    <model>rb_irfmn</model>
    <!--  QSAR classification model for Ready Biodegradability ver.1.0.9
-->
    <model>logp_meylan</model>
    <!--  LogP model based on Meylan work (and implemented in EPI Suite software as KowWin). ver.1.1.4
-->
  </models>
  <output>
  <!-- 
    <multipleTXT>out</multipleTXT>
    -->
    <singleTXT>%s</singleTXT>
  </output>
</vega>